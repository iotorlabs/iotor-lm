{
  "command": "update",
  "description": "Updates installed libraries to their newest version according to library.json.",
  "usage": [
    "update <name> [<name> ..] [<options>]"
  ],
  "options": [
    {
      "shorthand": "-F",
      "flag": "--force-latest",
      "description": "Force latest version on conflict"
    },
    {
      "shorthand": "-h",
      "flag": "--help",
      "description": "Show this help message"
    },
    {
      "shorthand": "-p",
      "flag": "--production",
      "description": "Do not install project devDependencies"
    },
    {
      "shorthand": "-S",
      "flag": "--save",
      "description": "Update dependencies in library.json"
    },
    {
      "shorthand": "-D",
      "flag": "--save-dev",
      "description": "Update devDependencies in library.json"
    }
  ]
}
