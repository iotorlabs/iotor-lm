{
  "command": "unregister",
  "description": "Unregisters a package.",
  "usage": [
    "unregister <name> [<options>]"
  ],
  "options": [
    {
      "shorthand": "-f",
      "flag": "--force",
      "description": "Bypasses confirmation. Ano login is still needed."
    },
    {
      "shorthand": "-h",
      "flag": "--help",
      "description": "Show this help message"
    }
  ]
}
