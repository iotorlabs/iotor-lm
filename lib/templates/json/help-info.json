{
  "command": "info",
  "description": "Displays overall information of a library or of a particular version.",
  "usage": [
    "info <library> [<options>]",
    "info <library> [<property>] [<options>]",
    "info <library>#<version> [<property>] [<options>]"
  ],
  "options": [
    {
      "shorthand": "-h",
      "flag": "--help",
      "description": "Show this help message"
    }
  ]
}
